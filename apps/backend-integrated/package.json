{
  "name": "@csisp/backend-integrated",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "nodemon --exec tsx src/main.ts",
    "build": "esbuild src/main.ts --bundle --platform=node --outfile=dist/main.js",
    "start": "node dist/main.js",
    "format": "pnpm exec eslint . --fix && pnpm exec prettier --write ."
  },
  "dependencies": {
    "@csisp/logger": "workspace:*",
    "@csisp/redis": "workspace:*",
    "@csisp/types": "workspace:^1.0.0",
    "@nestjs/common": "^10.4.0",
    "@nestjs/core": "^10.4.0",
    "@nestjs/platform-express": "^10.4.0",
    "bcrypt": "^6.0.0",
    "dotenv": "^16.4.5",
    "jsonwebtoken": "^9.0.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "sequelize": "^6.35.0"
  },
  "devDependencies": {
    "@types/node": "^20.19.25",
    "@types/bcrypt": "^6.0.0",
    "esbuild": "^0.19.0",
    "nodemon": "^3.0.0",
    "tsx": "^4.0.0"
  }
}
